{
  "name": "@hitarashi/trakt",
  "version": "0.1.0",
  "exports": {
    ".": "./src/main.ts",
    "./types": "./src/types/index.ts",
    "./types/movies": "./src/types/movies.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/**/*.ts"
    ],
    "exclude": [
      "tests"
    ]
  },
  "tasks": {
    "dev": "deno run --watch src/main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/expect": "jsr:@std/expect@^1.0.16",
    "@std/testing": "jsr:@std/testing@^1.0.14",
    "ky": "npm:ky@^1.8.1"
  },
  "test": {
    "include": [
      "tests/**/*.ts"
    ]
  }
}
